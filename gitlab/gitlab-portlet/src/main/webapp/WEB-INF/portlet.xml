<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>CommitHistory</portlet-name>
		<display-name>CommitHistory</display-name>
		<portlet-class>
			javax.portlet.faces.GenericFacesPortlet
		</portlet-class>
		<init-param>
			<name>javax.portlet.faces.bridgeEventHandler</name>
			<value>de.hska.wi.awp.datasource.event.StudentHistoryEventHandler</value>
		</init-param>
		<init-param>
			<name>javax.portlet.faces.defaultViewId.view</name>
			<value>/views/commitHistory/commitHistoryViewMode.xhtml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>GitLab - Commit History (SP)</title>
			<short-title>Commit History</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-processing-event>
    		<qname xmlns:x="http://liferay.com/events">x:ipc.studentSelected</qname>
		</supported-processing-event>
		<supported-processing-event>
    		<qname xmlns:x="http://liferay.com/events">x:ipc.projectSelected</qname>
		</supported-processing-event>
	</portlet>
	<portlet>
		<portlet-name>GitLab Load Data</portlet-name>
		<display-name>GitLab Load Data</display-name>
		<portlet-class>javax.portlet.faces.GenericFacesPortlet</portlet-class>
		<init-param>
			<name>javax.portlet.faces.defaultViewId.view</name>
			<value>/views/loadData/view.xhtml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Daten GitLab laden</title>
			<short-title>Daten GitLab laden</short-title>
			<keywords>Daten GitLab laden</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<event-definition>
    	<qname xmlns:x="http://liferay.com/events">x:ipc.studentSelected</qname>
    	<value-type>java.lang.String</value-type>
    </event-definition>
    <event-definition>
    	<qname xmlns:x="http://liferay.com/events">x:ipc.projectSelected</qname>
    	<value-type>java.lang.String</value-type>
    </event-definition>
</portlet-app>